{
  "name": "@mihoyo-kit/checkin",
  "version": "0.2.3",
  "license": "MIT",
  "homepage": "https://github.com/leonismoe/genshin-stats/tree/main/packages/checkin",
  "repository": "github:leonismoe/genshin-stats",
  "sideEffects": false,
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.mjs",
      "require": "./lib/index.js",
      "default": "./lib/index.js"
    },
    "./package.json": "./package.json",
    "./types": "./lib/types.d.ts",
    "./lib/*": {
      "types": "./lib/*.d.ts",
      "import": "./lib/*.mjs",
      "require": "./lib/*.js",
      "default": "./lib/*.js"
    }
  },
  "files": [
    "bin",
    "lib",
    "README.md"
  ],
  "bin": {
    "checkin-honkai3rd-cn": "lib/honkai3rd/bin/checkin-cn.mjs",
    "checkin-genshin-cn": "lib/genshin/bin/checkin-cn.mjs"
  },
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p tsconfig.build.json -m es2015 && node post-build.mjs mjs",
    "build:cjs": "tsc -p tsconfig.build.json -m commonjs -t es5 -d && node post-build.mjs",
    "clean": "rimraf lib pkg-temp *.tsbuildinfo",
    "prepublishOnly": "rimraf ./pkg-temp && clean-publish",
    "postpublish": "rimraf ./pkg-temp"
  },
  "dependencies": {
    "@mihoyo-kit/api": "workspace:^",
    "undici": "^5.8.0"
  },
  "optionalDependencies": {
    "find-chrome-bin": "^1.0.2",
    "puppeteer-core": "^16.2.0"
  },
  "devDependencies": {
    "@types/node": "^18.7.11",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "publishConfig": {
    "types": "lib/index.d.ts",
    "directory": "pkg-temp",
    "access": "public"
  }
}
