{
  "name": "@mihoyo-kit/genshin-api",
  "version": "0.3.0",
  "license": "MIT",
  "description": "Provides some encapsulated utilities function for Genshin Impact.",
  "homepage": "https://github.com/leonismoe/genshin-stats/tree/main/packages/genshin-api",
  "repository": "github:leonismoe/genshin-stats",
  "sideEffects": false,
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "lib/index.d.ts",
  "browser": {
    ".": "dist/browser.umd.js",
    "./dist/index.esm.js": "dist/browser.esm.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "default": "./dist/browser.umd.js"
    },
    "./package.json": "./package.json",
    "./types": "./lib/types/index.d.ts",
    "./typings": "./lib/types/index.d.ts",
    "./lib/types": "./lib/types/index.d.ts",
    "./lib/typings": "./lib/types/index.d.ts",
    "./lib/*": {
      "import": "./lib/*.mjs",
      "require": "./lib/*.js",
      "default": "./lib/*.js"
    },
    "./dist/*": "./dist/*.js"
  },
  "files": [
    "*.d.ts",
    "*.json",
    "dist",
    "lib",
    "README.md"
  ],
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs && npm run bundle",
    "build:esm": "tsc -m es2015 && node post-build.mjs mjs",
    "build:cjs": "tsc -m commonjs -d && node post-build.mjs",
    "bundle": "rollup -c rollup.config.js",
    "clean": "rimraf lib",
    "ts-node": "node --loader ts-node/esm --conditions=ts-node --experimental-specifier-resolution=node",
    "prepublishOnly": "rimraf ./pkg-temp && clean-publish",
    "postpublish": "rimraf ./pkg-temp"
  },
  "peerDependencies": {
    "@mihoyo-kit/api": "^0.3.10",
    "@mihoyo-kit/genshin-data": "^2.6.0",
    "undici": "^5.0.0"
  },
  "devDependencies": {
    "@mihoyo-kit/api": "workspace:^",
    "@mihoyo-kit/genshin-data": "workspace:^",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.2",
    "globby": "^13.1.1",
    "mkdirp": "^1.0.4",
    "rollup": "^2.75.0",
    "tslib": "^2.4.0",
    "typescript": "^4.7.2"
  },
  "publishConfig": {
    "directory": "pkg-temp",
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
